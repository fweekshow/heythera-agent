{
  "name": "basecamp-concierge-agent",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:examples": "echo 'skip examples build'",
    "clean": "rimraf node_modules && npm run clean:dbs",
    "clean:dbs": "rimraf *.db3* ||:",
    "dev": "npx --yes tsx --env-file .env --watch src/index.ts",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "gen:keys": "npx --yes tsx src/services/scripts/generateKeys.ts",
    "lint": "eslint .",
    "revoke": "tsx src/services/scripts/revokeInstallations.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc"
  },
  "resolutions": {
    "@xmtp/node-sdk": "4.0.3"
  },
  "dependencies": {
    "@bankr/sdk": "^0.1.0-alpha.5",
    "@langchain/core": "^0.3.72",
    "@langchain/openai": "^0.6.9",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@xmtp/content-type-transaction-reference": "^2.0.2",
    "@xmtp/content-type-wallet-send-calls": "^2.0.0",
    "@xmtp/node-sdk": "4.0.3",
    "better-sqlite3": "^11.7.0",
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "hardhat": "^2.26.3",
    "langchain": "^0.3.31",
    "luxon": "^3.5.0",
    "uint8arrays": "^5.1.0",
    "viem": "^2.22.17",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.19.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/eslint__js": "^8.42.3",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.13.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.8",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "engines": {
    "node": ">=20 <24"
  }
}
